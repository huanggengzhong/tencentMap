.footer {
  display: flex;
  /* margin-bottom:10rpx; */
  position: fixed;
  z-index: 999;
  bottom: 0;
  background-color: #fff;
}

.footer .child1 {
  width: 30%;
  text-align: center;
  font-size: 20rpx;
}

.footer .child2 {
  width: 60%;
  display: flex;
  align-items: center;
}

.footer .child3 {
  width: 60%;
  display: flex;
  align-items: center;
  margin-left: 20px;
}

.footer image {
  width: 30rpx;
  height: 30rpx;
  margin-top: 20rpx;
}

.footer button {
  width: 264rpx;
  font-size: 32rpx;
  color: white;
}

.footer .child2 button {
  background-color: #09d488;
}

.footer .child2 button::after {
  border: none;
}

.footer .child3 button::after {
  border: none;
}

.footer .child3 button {
  background-color: #329dfa;
  margin-right: 40rpx;
}

.my-content .my-map {
  height: 338rpx;
}

/* 户型滚动 */

.tab-h {
  height: 336rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
}

.tab-item {
  display: inline-block;
}

.tab-item:last-child .my-photo {
  margin-right: 0rpx;
}

.child1 .share {
  width: 80rpx;
  height: 120rpx;
  background-color: #fff;
  padding: 0;
  border: none;
  outline: none;
  margin-left: 62rpx;
}

.child1 .share::after {
  border: none;
}

.child1 .share image {
  width: 40rpx;
  display: block;
  height: 40rpx;
  text-align: center;
}

.child1 .share view {
  width: 80rpx;
  color: #000;
  font-size: 24rpx;
  text-align: left;
}

.zhoubian {
  margin-bottom: 130px;
}

.my-image {
  width: 100%;
  height: 500rpx;
}

.my-button {
  position: absolute;
  bottom: 45rpx;
  right: 45rpx;
  /* width:183rpx; */
  height: 66rpx;
  line-height: 66rpx;
  border-radius: 50rpx;
  background-color: #e1e1e1;
  color: #535659;
  /* font-weight:bold; */
  opacity: 0.8;
}

.my-button::after {
  border: none;
}

.pageContent {
  padding-bottom: 250rpx;
  left: -780rpx;
  position: absolute;
  width: 750rpx;
  background: #f7f7ff;
  overflow-x: hidden;
}
