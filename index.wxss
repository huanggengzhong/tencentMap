.nav{
position:fixed;
top:0;
width:100%;
z-index:9999;

background-color:#fff;

}


.my-swiper {
  height: 500rpx;
  position: relative;
}

swiper {
  height: 500rpx;
}

.slide-image {
  width: 100%;
  height: 500rpx;
}

.imageCount {
  width: 80rpx;
  height: 60rpx;
  border-radius: 50%;
  line-height: 58rpx;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
  position: absolute;
  right: 26rpx;
  bottom: 20rpx;
}

.loupan {
  padding: 40rpx;
  /* background-color: #ccc; *//* height: 600rpx; */
  margin-top: 30rpx;
  box-shadow: 0px 20rpx 0px #f7f8fa;
}

.loupan-title {
  font-size: 44rpx;
  font-weight: 600;
}

.loupan-child1 {
  display: flex;
  justify-content: flex-start;
  margin-top: 40rpx;
}

.loupan-child1 text {
  margin-right: 10rpx;
  font-size: 28rpx;
  border-radius: 6rpx;
  padding: 3rpx;
  padding-left: 8rpx;
  padding-right: 8rpx;
}

.loupan-child1 .green {
  background-color: #09d488;
  color: #fff;
}

.loupan-child1 .gray {
  background-color: #fff;
  color: #888;
  border: 1px solid #888;
}

.loupan-child2, .loupan-child3, .loupan-child4 {
  color: #969aa1;
  font-size: 28rpx;
  font-weight: 600;
}

.loupan-child2 {
  margin-top: 60rpx;
  font-size: 32rpx;
}

.loupan-child3, .loupan-child4 {
  margin-top: 30rpx;
}

.loupan-child3 {
  display: flex;
  justify-content: flex-start;
}

.loupan-child3 view {
  width: 50%;
}

.num1 {
  color: #e85b14;
}

.num {
  position: relative;
}

.num::after {
  content: '2';
  font-size: 15rpx;
  position: absolute;
  top: 0px;
}

.blue {
  color: #3ca2fa;
}

.space {
  margin-left: 20rpx;
}

.my-btn {
  background-color: #e1eefa;
  color: #3ca2fa;
  margin-top: 60rpx;
}

.my-btn::after {
  border: none;
}

.my-padding {
  padding: 40rpx;
}

.goufang {
  margin-top: 20rpx;
  box-shadow: 0px 10rpx 0px #f7f8fa;
}

.my-title {
  font-size: 32rpx;
  font-weight: 600;
}

.my-content {
  margin-top: 20rpx;
}

.goufang .my-content {
  font-size: 24rpx;
  color: #888;
}

.goufang .my-content .my-yellow {
  color: #e85b14;
}

.jianfang {
  margin-top: 10rpx;
  box-shadow: 0px 20rpx 0px #f7f8fa;
}

.jianfang .my-title text {
  font-size: 24rpx;
  color: #ccc;
  font-weight: 400;
}

.jianfang .my-content view {
  font-size: 24rpx;
  color: #888;
}

.jianfang .my-content .my-margin {
  margin-top: 20rpx;
}

.jianfang .my-content view text {
  color: #e85b14;
}

.huxing {
  box-shadow: 0px 20rpx 0px #f7f8fa;
}

.huxing .my-content {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
}

.huxing .my-photo {
  margin-right: 20rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
}

.huxing .my-photo image {
  display: block;
  width: 242rpx;
  height: 210rpx;
  /* border:1px dashed #888; */
  box-sizing: border-box;
}

.huxing .my-photo .my-imageinfo {
  border-radius: 0 0 10rpx 10rpx;
  padding-left: 16rpx;
}

.huxing .my-photo .my-imageinfo .child1 {
  font-size: 24rpx;
}

.huxing .my-photo .my-imageinfo .child2, .huxing .my-photo .my-imageinfo .child3 {
  font-size: 22rpx;
  margin-top: 8rpx;
}

.huxing .my-photo .my-imageinfo .child2 text {
  position: relative;
}

/* .huxing .my-photo .my-imageinfo .child2 text::after{
  content: '2';
  font-size: 15rpx;
  position: absolute;
  top: 0px;
} */

.huxing .my-photo .my-imageinfo .child3 {
  margin-bottom: 8rpx;
}

.huxing .my-photo .my-imageinfo .child3 text {
  background-color: #09d488;
  color: #fff;
  padding: 0 4rpx;
  text-align: center;
  line-height: 22rpx;
  height: 22rpx;
}

.xiangmu {
  box-shadow: 0px 20rpx 0px #f7f8fa;
}

.xiangmu .my-content {
  font-size: 24rpx;
  color: #888;
  text-indent: 2em;
  line-height: 40rpx;
  text-align: justify;
}

/* .zhoubian {
  box-shadow: 0px 32rpx 0px #f7f8fa;
  
} */

.zhoubian .my-content map {
  width: 100%;
  height: 338rpx !important;
}

.zhoubian .my-content .father {
  display: flex;
  padding-top: 40rpx;
  border-left: 1px solid #f3f3f3;
  border-right: 1px solid #f3f3f3;
  border-bottom: 1px solid #f3f3f3;
  border-radius: 0 0 10rpx 10rpx;
}

.zhoubian .my-content .father .child {
  width: 25%;
  text-align: center;
  margin-bottom: 40rpx;
}

.zhoubian .my-content .father .child image {
  width: 54rpx;
  height: 54rpx;
}

.zhoubian .my-content .father .child view {
  font-size: 28rpx;
}

